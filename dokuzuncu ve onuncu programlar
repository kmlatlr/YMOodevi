package sınıff;

public class projjjeee3 {

	public static void main(String[] args) {

		
    int sayac = 0;
		
		
		while(sayac<100) {
			//Döngünün gövdesi
			System.out.println("Bilgisayar mühendisliği oryantasyonu");
			sayac++;
			
		}
		int top = 0, ii=1;
		
		while(ii<10) {
		top = top+ii;
		ii++;
		}
		
		
		
		final double KILOMETRE_MIL_SAYISI = 1.609;
		double mil = 6986586;
		double kilometre = (mil*KILOMETRE_MIL_SAYISI);
		System.out.println("100 mil = " + kilometre + " kilometreye eşittir.");
		
	
	}
	
}


